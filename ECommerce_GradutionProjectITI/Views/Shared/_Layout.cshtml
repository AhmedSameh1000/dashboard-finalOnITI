<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ECommerce_GradutionProjectITI</title>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>E-Commerce Shopping</title>
        <!-- Font Icon And Main Stlyling CSS File -->
        <link rel="stylesheet"
              href="https://unpkg.com/boxicons@latest/css/boxicons.min.css" />
        <link rel="stylesheet" href="css/all.min.css" />
        <link rel="stylesheet" href="css/normalize.css" />
        <link rel="stylesheet" href="css/style.css" />
        <!-- <link rel="preconnect" href="https://fonts.googleapis.com" />
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
        <link href="https://fonts.googleapis.com/css2?family=Poppins&display=swap" rel="stylesheet" /> -->
        <link rel="preconnect" href="https://fonts.googleapis.com" />
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
        <link href="https://fonts.googleapis.com/css2?family=Noto+Serif:ital@1&family=Open+Sans:wght@400;500;600;700&family=Quicksand&display=swap"
              rel="stylesheet" />
        <!-- Seo Kayword and Description Website -->
        <meta name="keywords" content="Furniture - eCommerce" />
        <meta name="description"
              content="E-Commerce for Furniture Selling and Shipping" />
        <meta name="author" content="ITI Team Members " />
        <script src="https://unpkg.com/boxicons@2.1.3/dist/boxicons.js"></script>
    </head>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header class="header">
        <div class="header-top">
            <div class="container">
                <!-- Header Left -->
                <div class="header-left">
                    <p>All collection of Furniture already available.</p>
                    <a class="icon" href="#">&nbsp;Read more ...</a>
                </div>
                <!-- End .header-left -->
                <!-- Start Header Right -->
                <div class="header-right">
                    <a class="icon" id="icon" href="login.html" data-toggle="modal">Sign in / Sign up</a>
                </div>
                <!-- End .header-right -->
            </div>
            <!-- End .container -->
        </div>
        <!-- End .header-top -->
        <div class="header-middle">
            <div class="container">
                <!-- Start header Logo -->
                <div class="header-left">
                    <a href="index.html" class="logo">
                        <img src="images/logo.jfif" alt="Logo" width="105" height="25" />
                    </a>
                </div>
                <!-- End header Logo -->
                <!-- Header Search -->
                <div class="header-center">
                    <div class="header-search">
                        <form action="#" method="get">
                            <div class="input-search">
                                <input onfocus="changeBorder()"
                                       onblur="resetToDefault()"
                                       type="search"
                                       class="form-control"
                                       name="q"
                                       id="q"
                                       placeholder="Search product ..." />
                                <button class="icon-search" type="submit">
                                    <box-icon name="search-alt-2"></box-icon>
                                </button>
                            </div>
                            <!-- End .header-search-wrapper -->
                        </form>
                    </div>
                    <!-- End .header-search -->
                </div>
                <div class="header-setting">
                    <div class="header-right">
                        <div class="account">
                            <box-icon class="icon" name="user"></box-icon>
                            <p>Account</p>
                        </div>
                        <div class="wishlist">
                            <box-icon class="icon" name="heart"></box-icon>
                            <span class="count-wishlist">3</span>
                            <p>Wishlist</p>
                        </div>
                        <div class="cart">
                            <a href="cart.html">
                                <box-icon class="icon" name="cart-alt"></box-icon>
                            </a>
                            <span class="count-cart"></span>
                            <p>Cart</p>
                        </div>
                    </div>
                    <!-- End .header-search -->
                </div>
            </div>
        </div>
        <div class="navbar">
            <div class="container">
                <div class="header-left">
                    <div class="menubar">
                        <span><box-icon id="menu" name="menu"></box-icon></span> Browse
                        Categories
                    </div>
                </div>
                <div class="header-middle">
                    <ul class="navlist">
                        <li><a class="focus" href="index.html">Home</a></li>
                        <li><a href="Product.html">SHOP</a></li>
                        <li><a href="#link">CATALOGUE</a></li>
                        <li><a href="mostsales.html">Most Sales</a></li>
                        <li><a href="contact us.html">CONTACT</a></li>
                        <li><a href="members.html">ABOUT US</a></li>
                    </ul>
                </div>
                <div class="header-right">
                    <p class="head-right">
                        <span><box-icon type="solid" name="hourglass-top"></box-icon></span><span>Clearance</span>
                        Up to 30% Off
                    </p>
                </div>
            </div>
        </div>
        <div class="fixednavbar"></div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="footer">
        <div class="container">
            <div class="footerleft">
                <img class="logo" src="images/logo.jfif" />
                <div>
                    <p>
                        Praesent dapibus, neque id cursus ucibus, tortor neque egestas
                        augue,<br />
                        eu vulputate magna eros eu erat.
                    </p>
                    <a href="#"><i class="fa-brands fa-facebook"></i></a>
                    <a href="#"><i class="fa-brands fa-twitter"></i></a>
                    <a href="#"><i class="fa-brands fa-instagram"></i></a>
                    <a href="#"><i class="fa-brands fa-youtube"></i></a>

                    <div class="Clear"></div>
                </div>
            </div>

            <div class="footerright">
                <div class="AboutUs">
                    <h4>Information</h4>
                    <ul>
                        <li><a href="about-2.html">About </a></li>
                        <li><a href="#">How to shop </a></li>
                        <li><a href="#">FAQ</a></li>
                        <li><a href="contact-2.html">Contact us</a></li>
                        <li><a href="login.html">Login</a></li>
                    </ul>
                </div>

                <div class="AboutUs">
                    <h4>Customer Services</h4>
                    <ul>
                        <li><a href="#">Payment Methods</a></li>
                        <li><a href="#">How to shop</a></li>
                        <li><a href="#">Money-back guarantee!</a></li>
                        <li><a href="#">Returns</a></li>
                    </ul>
                </div>

                <div class="AboutUs">
                    <h4>My Account</h4>
                    <ul>
                        <li><a href="#">Sign In</a></li>
                        <li><a href="cart.html">View Cart</a></li>
                        <li><a href="#">My Wishlist</a></li>
                        <li><a href="#">Track My Order</a></li>
                        <li><a href="#">Help</a></li>
                    </ul>
                </div>
            </div>

            <div class="Clear"></div>
        </div>

        <hr />
        <div class="container">
            <div class="left">
                <p style="display: inline-block;">
                    Copyright © 2022 Store. All Rights Reserved.
                </p>
            </div>
            <div class="right">
                <img src="images/payments.png" />
            </div>

            <div class="Clear"></div>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
